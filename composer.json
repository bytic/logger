{
  "name": "bytic/logger",
  "description": "Logger component for bytic",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Logger\\": "src/"
    },
    "classmap": [
      "legacy"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Logger\\Tests\\": "tests/src",
      "Nip\\Logger\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.0",
    "monolog/monolog": "^1.20"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "^7.2",
    "mockery/mockery": "^1.1"
  }
}
